{
  "manifest_version": 2,
  "name": "Oracle Fusion Enhancer",
  "version": "0.3.0",
  "description": "Oracle Fusion enhancer for users",
  "homepage_url": "https://github.com/macmorning/oab-fusion-plus",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.oraclecloud.com/*"],
      "js": ["enhance.js"]
    }
  ],
  "permissions": ["storage"]
}